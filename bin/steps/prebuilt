# Installing prebuilt binaries. Called from bin/compile.
puts-step "Installing prebuilt binaries."

bpwatch start prebuilt_install

# Syntax sugar.
source $BIN_DIR/utils

PREBUILD_VENDORS=$(dirname $BIN_DIR)/vendor

echo $(ls -laF /app)

echo "Expanding prebuilt binaries..."
#tar -xzvf $PREBUILD_VENDORS/numpy-1.9.2.linux-i686.tar.gz -C /app/.heroku/python/lib/python2.7 &> /dev/null
pip install $PREBUILD_VENDORS/numpy-1.9.2.linux-i686.tar.gz

echo "Testing numpy..."
python -c "import numpy"

bpwatch stop prebuilt_install

