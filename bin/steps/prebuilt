# Installing prebuilt binaries. Called from bin/compile.
puts-step "Installing prebuilt binaries."

bpwatch start prebuilt_install

# Syntax sugar.
source $BIN_DIR/utils

echo "Python version:"
echo $(python --version)

PREBUILD_VENDORS=/app/.heroku/vendor
PREFIX=$(python -c "import sys; print sys.prefix")

mv /app/.heroku/python/vagrant/numpy/operand_venv/lib/python2.7/site-packages/numpy* /app/.heroku/python/lib/python2.7/site-packages

echo ".heroku contents:"
find /app/.heroku | egrep numpy

echo "Expanding prebuilt binaries..."
#tar -xzvf $PREBUILD_VENDORS/numpy-1.9.2.linux-x86_64.tar.gz -C $PREFIX # &> /dev/null

bpwatch stop prebuilt_install

